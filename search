    final List<String> filteredTitles = tripsData
        .where((trip) => trip.title.toLowerCase().contains(query.toLowerCase()))
        .map((trip) => trip.title)
        .toList();

    return filteredTitles;
  }
}

void _navigateToItemDetails(BuildContext context, String suggestion) {
  // You can replace the MaterialPageRoute with your own navigation logic
  Navigator.of(context).push(
    MaterialPageRoute(
      builder: (context) => ItemDetailsScreen(title: suggestion),
    ),
  );
}

class ItemDetailsScreen extends StatelessWidget {
  final String title;

  const ItemDetailsScreen({Key? key, required this.title}) : super(key: key);

  @override
  Widget build(BuildContext context) {
    // Implement your item details screen here, using the provided title
    return Scaffold(
      appBar: AppBar(
        title: Text(title),
      ),
      body: Center(
        child: Column(
          mainAxisAlignment: MainAxisAlignment.center,
          children: [
            Text('Item $title is found'),
            const SizedBox(height: 16),
            ElevatedButton(
              onPressed: () {
                Navigator.of(context).pushNamed('/');
              },
              child: const Text('Go to Home'),
            ),
          ],
        ),
      ),
    );
  }
}
